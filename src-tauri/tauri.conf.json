{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": {},
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "The Finals Eclipper",
  "mainBinaryName": "the-finals-eclipper",
  "version": "0.0.6",
  "identifier": "dev.nic96.the-finals-eclipper",
  "plugins": {},
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "THE FINALS Eclipper",
        "width": 800,
        "height": 600,
        "additionalBrowserArgs": "--disable-features=msWebOOUI,msPdfOOUI,msSmartScreenProtection",
        "useHttpsScheme": true
      }
    ],
    "security": {
      "csp": null
    }
  }
}
